pipeline {
    agent any 
    stages {
        stage('wakeup docker image') { 
            steps { 
                sh 'ls -la' 
            }
        }
        stage('do unitests over docker image'){
            steps {
                sh 'ls -la'
            }
        }
        stage('do feature tests over docker image'){
            steps {
                sh 'ls -la'
            }
        }
        stage('do load tests over docker image'){
            steps {
                sh 'ls -la'
            }
        }
        stage('deploy to qa') {
            steps {
                sh 'ls -la'
            }
        }
        stage('send slack message over docker image') {
            steps {
                sh 'ls -la'
            }
        }
    }
}
